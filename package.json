  "license": "MIT",
  "contributors": [
    {
      "name": "Jean-Baptiste Thery",
      "email": "jb.thery@flameapp.fr",
      "url": "https://flameapp.studio"
    },
    {
      "name": "Sandra Sain-Julien",
      "email": "sandra.saint.julien@gmail.com"
