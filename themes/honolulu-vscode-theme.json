{
  "name": "Honolulu theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#100d16",
    "activityBar.border": "#3e3a51c1",
    "activityBar.inactiveForeground": "#818b97",
    "activityBarBadge.background": "#FF7AB2",
    "activityBarBadge.foreground": "#ffffff",

    "badge.background": "#FF7AB2",
    "badge.foreground": "#ffffff",

    "button.background": "#2b2a38",
    "button.hoverBackground": "#363447",
    "button.secondaryBackground": "#2b2a38",
    "button.secondaryHoverBackground": "#363447",

    "debugToolBar.background": "#21202b",
    "debugToolBar.border": "#3e3a51c1",

    "diffEditor.border": "#3e3a51c1",
    "diffEditor.insertedTextBackground": "#5DD8FF1e",
    "diffEditor.removedTextBackground": "#de090976",

    "editor.background": "#100d16",
    "editor.foreground": "#eeffff",
    "editor.lineHighlightBackground": "#302e3f91",
    "editor.lineHighlightBorder": "#de090900",
    "editor.selectionBackground": "#FF7AB236",

    "editorCodeLens.foreground": "#818b97",

    "editorCursor.background": "#ffffff",
    "editorCursor.foreground": "#FF7AB2",

    "editorGroup.border": "#3e3a51c1",

    "editorGroupHeader.tabsBackground": "#100d16",

    "editorGutter.addedBackground": "red",
    "editorGutter.deletedBackground": "#de0909",
    "editorGutter.foreground": "#818b97",
    "editorGutter.modifiedBackground": "#FF7AB2",

    "editorHoverWidget.background": "#2b2a38",
    "editorHoverWidget.border": "#3e3a51c1",

    "editorIndentGuide.activeBackground": "#FF7AB274",
    "editorIndentGuide.background": "#3e3a51c1",

    "editorLineNumber.activeForeground": "#e2eaf5",
    "editorLineNumber.foreground": "#818b97",

    "editorWidget.background": "#21202b",
    "editorWidget.resizeBorder": "#FF7AB2",

    "focusBorder": "#FF7AB2",

    "foreground": "#e2eaf5",

    "gitDecoration.addedResourceForeground": "red",
    "gitDecoration.conflictingResourceForeground": "#6c6cc4",
    "gitDecoration.deletedResourceForeground": "#FF5370",
    "gitDecoration.ignoredResourceForeground": "#818b97",
    "gitDecoration.modifiedResourceForeground": "#ffc9aa",
    "gitDecoration.stageDeletedResourceForeground": "#d5bcfa",
    "gitDecoration.stageModifiedResourceForeground": "#ffc9aa",
    "gitDecoration.submoduleResourceForeground": "#8db9e2",
    "gitDecoration.untrackedResourceForeground": "#5DD8FF",

    "input.background": "#302e3f91",

    "list.activeSelectionBackground": "#100d16",
    "list.dropBackground": "#100d16",
    "list.focusBackground": "#100d16",
    "list.highlightForeground": "#FF7AB2",
    "list.hoverBackground": "#100d16",
    "list.inactiveSelectionBackground": "#100d16",

    "listFilterWidget.background": "#100d16",

    "notifications.border": "#3e3a51c1",

    "panel.border": "#3e3a51c1",

    "peekView.border": "#FF7AB2",

    "peekViewTitle.background": "#100d16",

    "scrollbar.shadow": "#17161d",
    "scrollbarSlider.activeBackground": "#3b394b",
    "scrollbarSlider.background": "#21202b",
    "scrollbarSlider.hoverBackground": "#3b394b",

    "selection.background": "#FF7AB236",

    "sideBar.background": "#100d16",
    "sideBar.border": "#3e3a51c1",

    "sideBarSectionHeader.background": "#100d16",
    "sideBarSectionHeader.border": "#3e3a51c1",

    "statusBar.background": "#100d16",
    "statusBar.border": "#3e3a51c1",
    "statusBar.debuggingBackground": "#100d16",
    "statusBar.noFolderBackground": "#100d16",

    "statusBarItem.remoteBackground": "#ff7ab2",

    "tab.activeBackground": "#100d16",
    "tab.activeBorder": "#100d16",
    "tab.activeBorderTop": "#FF7AB2",
    "tab.activeForeground": "#e2eaf5",
    "tab.border": "#3e3a51c1",
    "tab.inactiveBackground": "#100d16",
    "tab.inactiveBorder": "#3e3a51c1",
    "tab.inactiveForeground": "#ffc9aa",
    "tab.unfocusedActiveBorder": "#3e3a51c1",
    "tab.unfocusedActiveBorderTop": "#3e3a51c1",

    "terminal.border": "#3e3a51c1",

    "textLink.activeForeground": "#f7adcc",
    "textLink.foreground": "#FF7AB2",

    "titleBar.activeBackground": "#100d16",
    "titleBar.activeForeground": "#e2eaf5",
    "titleBar.border": "#3e3a51c1",
    "titleBar.inactiveBackground": "#1d1c25",
    "titleBar.inactiveForeground": "#818b97",

    "tree.indentGuidesStroke": "#3e3a51c1",

    "widget.shadow": "#17161d"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#a691f2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#5DD8FF"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#ffa660"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#FF7AB2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#FF7AB2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": ["string", "string punctuation.section.embedded source"],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#a691f2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#FF7AB2",
        "foreground": "#f0f6fc",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ffad60"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#ffad60"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#a691f2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#ffad60"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#490202",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#FF7AB2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#04260f",
        "foreground": "#ffad55"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#5a1e02",
        "foreground": "#a691f2"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#161b22",
        "background": "#d6bcfa"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#5DD8FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#d6bcfa"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#8b949e"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#ffc9aa",
        "fontStyle": "underline"
      }
    }
  ]
}
