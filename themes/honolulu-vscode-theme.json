    "activityBarBadge.background": "#ee81b0",
    "activityBarBadge.foreground": "#ffffff",
    "statusBar.border": "#3e3a51c1",
    "titleBar.border": "#3e3a51c1",
    "sideBar.border": "#3e3a51c1",
    "panel.border": "#3e3a51c1",
    "terminal.border": "#3e3a51c1",
    "notifications.border": "#3e3a51c1",
    "editorGroup.border": "#3e3a51c1",
    "tab.border": "#3e3a51c1",
    "peekView.border": "#ee81b0",
    "peekViewTitle.background": "#21202a",
    "scrollbar.shadow": "#17161d",
    "widget.shadow": "#17161d",
    "tree.indentGuidesStroke": "#3e3a51c1",
    "editorIndentGuide.background": "#3e3a51c1",
    "editorIndentGuide.activeBackground": "#ee81b074",
    "list.inactiveSelectionBackground": "#21202a",
    "list.hoverBackground": "#21202a",
    "list.activeSelectionBackground": "#21202a",
    "list.dropBackground": "#21202a",
    "list.highlightForeground": "#ee81b0",
    "list.focusBackground": "#21202a",
    "listFilterWidget.background": "#21202a",
    "scrollbarSlider.background": "#21202a",
    "scrollbarSlider.hoverBackground": "#3b394b",
    "scrollbarSlider.activeBackground": "#3b394b",
    "editorCursor.foreground": "#ee81b0",
    "editorCursor.background": "#ffffff",
    "editor.selectionBackground": "#ee81b036",
    "gitDecoration.addedResourceForeground": "#89e5fb",
    "gitDecoration.conflictingResourceForeground": "#6c6cc4",
    "gitDecoration.deletedResourceForeground": "#FF5370",
    "gitDecoration.ignoredResourceForeground": "#818b97",
    "gitDecoration.modifiedResourceForeground": "#ffc9aa",
    "gitDecoration.stageDeletedResourceForeground": "#d5bcfa",
    "gitDecoration.stageModifiedResourceForeground": "#ffc9aa",
    "gitDecoration.submoduleResourceForeground": "#8db9e2",
    "gitDecoration.untrackedResourceForeground": "#e2eaf5",
    "editorGutter.modifiedBackground": "#ee81b0",
    "editorGutter.addedBackground": "#62dead",
    "editorGutter.deletedBackground": "#de0909",
    "diffEditor.insertedTextBackground": "#62dead6c",
    "diffEditor.removedTextBackground": "#de090976",
    "diffEditor.border": "#3e3a51c1",
    "button.background": "#2b2a38",
    "button.hoverBackground": "#363447",
    "button.secondaryBackground": "#2b2a38",
    "button.secondaryHoverBackground": "#363447",
    "editorWidget.resizeBorder": "#ee81b0",
    "editor.lineHighlightBackground": "#302e3f91",
    "editor.lineHighlightBorder": "#de090900",
    "editorHoverWidget.background": "#2b2a38",
    "editorHoverWidget.border": "#3e3a51c1"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#818b97"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#ee81b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "keyword.control",
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag",
        "meta.tag.sgml",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#d5bcfa"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method"
      ],
      "settings": {
        "foreground": "#89e5fb"
      }
    },
    {
      "name": "Block Level Variables",
      "scope": ["meta.block variable.other"],
      "settings": {
        "foreground": "#d5bcfa"
      }
    },
    {
      "name": "Other Variable, String Link",
      "scope": ["support.other.variable", "string.other.link"],
      "settings": {
        "foreground": "#d5bcfa"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "variable.parameter",
        "keyword.other.unit",
        "keyword.other"
      ],
      "settings": {
        "foreground": "#F78C6C"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": [
        "string",
        "constant.other.symbol",
        "constant.other.key",
        "entity.other.inherited-class",
        "markup.heading",
        "markup.inserted.git_gutter",
        "meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "entity.name",
        "support.type",
        "support.class",
        "support.other.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types"
      ],
      "settings": {
        "foreground": "#775e9e"
      }
    },
    {
      "name": "Entity Types",
      "scope": ["support.type"],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name"
      ],
      "settings": {
        "foreground": "#B2CCD6"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Language methods",
      "scope": ["variable.language"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "entity.name.method.js",
      "scope": ["entity.name.method.js"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#89e5fb"
      }
    },
    {
      "name": "meta.method.js",
      "scope": [
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#89e5fb"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#ee81b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#775e9e"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#775e9e"
      }
    },
    {
      "name": "CSS ID's",
      "scope": ["source.sass keyword.control"],
      "settings": {
        "foreground": "#89e5fb"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#ee81b0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "Decorators",
      "scope": [
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#89e5fb"
      }
    },
    {
      "name": "ES7 Bind Operator",
      "scope": [
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF5370"
      }
    },
    {
      "name": "JSON Key - Level 0",
      "scope": [
        "source.json meta.structure.dictionary.json support.type.property-name.json"
      ],
      "settings": {
        "foreground": "#ee81b0",
        "fontStyle": "bold"
      }
    },
