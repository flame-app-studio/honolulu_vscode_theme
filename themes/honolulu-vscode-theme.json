{
  "name": "Honolulu theme",
  "type": "dark",
  "colors": {
    "activityBar.background": "#191919",
    "activityBar.border": "#252525",
    "activityBar.inactiveForeground": "#80878e",
    "activityBarBadge.background": "#a27ae3",
    "activityBarBadge.foreground": "#fff",

    "badge.background": "#a27ae3",
    "badge.foreground": "#fff",

    "button.background": "#a27ae3",
    "button.hoverBackground": "#d1b4fb",
    "button.secondaryBackground": "#252525",
    "button.secondaryHoverBackground": "#363447",

    "debugToolBar.background": "#252525",
    "debugToolBar.border": "#252525",

    "diffEditor.border": "#252525",

    "editor.background": "#191919",
    "editor.foreground": "#fff",
    "editor.lineHighlightBackground": "#30303091",
    "editor.lineHighlightBorder": "#de090900",
    "editor.selectionBackground": "#f7a0d336",

    "editorCodeLens.foreground": "#80878e",
    "editorCursor.background": "#fff",
    "editorCursor.foreground": "#f7a0d3",
    "editorGroup.border": "#252525",
    "editorGroupHeader.tabsBackground": "#191919",
    "editorHoverWidget.background": "#252525",
    "editorHoverWidget.border": "#303030",
    "editorIndentGuide.activeBackground": "#f7a0d3",
    "editorIndentGuide.background": "#303030",
    "editorLineNumber.activeForeground": "#808080",
    "editorLineNumber.foreground": "#505050",
    "editorWidget.background": "#252525",
    "editorWidget.resizeBorder": "#fff",

    "focusBorder": "#fff",

    "foreground": "#e2eaf5",

    "input.background": "#30303091",

    "list.activeSelectionBackground": "#252525",
    "list.dropBackground": "#191919",
    "list.focusBackground": "#191919",
    "list.highlightForeground": "#fff",
    "list.hoverBackground": "#191919",
    "list.inactiveSelectionBackground": "#252525",

    "listFilterWidget.background": "#191919",

    "notifications.border": "#252525",

    "panel.border": "#252525",

    "peekView.border": "#fff",
    "peekViewTitle.background": "#191919",

    "scrollbar.shadow": "#191919",
    "scrollbarSlider.activeBackground": "#303030",
    "scrollbarSlider.background": "#303030",
    "scrollbarSlider.hoverBackground": "#404040",

    "selection.background": "#f7a0d336",

    "sideBar.background": "#191919",
    "sideBar.border": "#252525",

    "sideBarSectionHeader.background": "#191919",
    "sideBarSectionHeader.border": "#252525",

    "statusBar.background": "#191919",
    "statusBar.border": "#252525",
    "statusBar.debuggingBackground": "#191919",
    "statusBar.noFolderBackground": "#191919",

    "statusBarItem.remoteBackground": "#a27ae3",

    "tab.activeBackground": "#191919",
    "tab.activeBorder": "#191919",
    "tab.activeBorderTop": "#fff",
    "tab.activeForeground": "#fff",
    "tab.border": "#252525",
    "tab.inactiveBackground": "#191919",
    "tab.inactiveBorder": "#252525",
    "tab.inactiveForeground": "#ffc9aa",
    "tab.unfocusedActiveBorder": "#252525",
    "tab.unfocusedActiveBorderTop": "#252525",

    "editorSuggestWidget.selectedBackground": "#f7a0d336",

    "terminal.border": "#252525",

    "textLink.activeForeground": "#d1b4fb",
    "textLink.foreground": "#a27ae3",

    "titleBar.activeBackground": "#191919",
    "titleBar.activeForeground": "#e2eaf5",
    "titleBar.border": "#252525",
    "titleBar.inactiveBackground": "#202020",
    "titleBar.inactiveForeground": "#80878e",

    "tree.indentGuidesStroke": "#303030",

    "widget.shadow": "#191919"
  },
  "semanticHighlighting": true,
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment", "string.comment"],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.other.constant",
        "variable.other.enummember",
        "variable.language",
        "entity"
      ],
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": [
        "entity.name",
        "meta.export.default",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#a27ae3"
      }
    },
    {
      "scope": [
        "variable.parameter.function",
        "meta.jsx.children",
        "meta.block",
        "meta.tag.attributes",
        "entity.name.constant",
        "meta.object.member",
        "meta.embedded.expression"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#7bedbb"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#ff9068"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#f7a0d3"
      }
    },
    {
      "scope": ["storage", "storage.type"],
      "settings": {
        "foreground": "#f7a0d3"
      }
    },
    {
      "scope": [
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": ["string", "string punctuation.section.embedded source"],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "meta.property-name",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#a27ae3"
      }
    },
    {
      "scope": "variable.other",
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "background": "#f7a0d3",
        "foreground": "#f0f6fc",
        "content": "^M"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": ["source.regexp", "string.regexp"],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repitition"
      ],
      "settings": {
        "foreground": "#ffc9aa"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#ffad60"
      }
    },
    {
      "scope": "support.constant",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "support.variable",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "foreground": "#ffad60"
      }
    },
    {
      "scope": "meta.module-reference",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#a27ae3"
      }
    },
    {
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#ffad60"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#c9d1d9"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "background": "#490202",
        "foreground": "#ffa198"
      }
    },
    {
      "scope": ["punctuation.section.embedded"],
      "settings": {
        "foreground": "#f7a0d3"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "background": "#04260f",
        "foreground": "#ffad55"
      }
    },
    {
      "scope": ["markup.changed", "punctuation.definition.changed"],
      "settings": {
        "background": "#5a1e02",
        "foreground": "#a27ae3"
      }
    },
    {
      "scope": ["markup.ignored", "markup.untracked"],
      "settings": {
        "foreground": "#161b22",
        "background": "#d1b4fb"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#7bedbb"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
        "foreground": "#d1b4fb"
      }
    },
    {
      "scope": [
        "brackethighlighter.tag",
        "brackethighlighter.curly",
        "brackethighlighter.round",
        "brackethighlighter.square",
        "brackethighlighter.angle",
        "brackethighlighter.quote"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "brackethighlighter.unmatched",
      "settings": {
        "foreground": "#ffa198"
      }
    },
    {
      "scope": ["constant.other.reference.link", "string.other.link"],
      "settings": {
        "foreground": "#ffc9aa",
        "fontStyle": "underline"
      }
    }
  ]
}
